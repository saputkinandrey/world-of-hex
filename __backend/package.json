{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for express with typescript, including logging, ORM  and database connection",
  "main": "index.js",
  "module": "es2015",
  "scripts": {
    "start": "node ./build/backend/src/index.js",
    "start:worker": "node ./build/src/workers.js",
    "start:migrate": "yarn knex migrate:latest && yarn start",
    "dev": "concurrently \"NODE_ENV=development nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "dev:worker": "nodemon -x ts-node src/workers.ts",
    "build": "tsoa spec-and-routes &&  tsc --project ./ && yarn copy-assets && yarn copy-email-templates && yarn copy-pdf-templates && yarn copy-kpi-queries && yarn copy-feedback-queries",
    "predev": "yarn build:routes",
    "prebuild": "rimraf build/ && yarn build:routes",
    "build:routes": "tsoa spec-and-routes",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "workers": "nodemon -x \"ts-node src/workers.ts\"",
    "copy-kpi-queries": "cp -r src/services/kpi/queries build/src/services/kpi/queries",
    "copy-feedback-queries": "cp -r src/services/feedback/queries build/src/services/feedback/queries",
    "copy-assets": "cp -r src/assets build/src/assets",
    "copy-email-templates": "cp -r src/email/templates build/src/email/templates",
    "copy-pdf-templates": "cp -r src/services/pdf/templates build/src/services/pdf/templates",
    "download-chrome": "yarn ts-node download-chrome.ts",
    "postinstall": "husky"
  },
  "keywords": [
    "express",
    "typescript",
    "orm",
    "database",
    "logging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^6.17.7",
    "@sentry/tracing": "^6.17.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/ms": "^0.7.31",
    "@types/node-schedule": "^1.3.2",
    "@types/opentok": "^2.12.0",
    "@types/slug": "^5.0.3",
    "@types/ua-parser-js": "^0.7.37",
    "adm-zip": "^0.5.10",
    "axios": "^1.0.0",
    "axios-mock-adapter": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-http-context": "^1.2.4",
    "express-jwt": "^8.4.1",
    "express-winston": "^4.2.0",
    "filereader": "^0.10.3",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.1",
    "html-to-text": "^8.2.1",
    "https": "^1.0.0",
    "ics": "^2.41.0",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "knex": "^2.4.2",
    "mjml": "^4.12.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.14.1",
    "nanoid": "^3.0.0",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.3",
    "objection": "^3.0.1",
    "opentok": "^2.13.0",
    "pg": "^8.7.1",
    "puppeteer-core": "^19.7.5",
    "reflect-metadata": "^0.1.10",
    "sanitize-filename": "^1.6.3",
    "slug": "^8.2.2",
    "sqs-consumer": "^7.0.2",
    "sqs-producer": "^3.1.1",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.1.2",
    "tsoa": "^5.1.1",
    "typeorm": "0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "ua-parser-js": "^1.0.36",
    "util": "^0.12.4",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "winston-logzio": "^5.1.3"
  },
  "devDependencies": {
    "@qte/eslint-config-typescript": "^1.0.3",
    "@types/adm-zip": "^0.5.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^8.1.0",
    "@types/jest": "^27.4.0",
    "@types/mjml": "^4.7.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.10",
    "@types/nodemailer": "^6.4.4",
    "@types/pg-connection-string": "^2.0.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^7.0.0",
    "date-fns-tz": "^2.0.0",
    "eslint": "^8.8.0",
    "husky": "^9.0.11",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "nodemon": "^2.0.15",
    "pg-connection-string": "^2.5.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
